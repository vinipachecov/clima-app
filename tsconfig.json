{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@main/*": [
        "./main/*"
      ],
      "@ui/*": [
        "./ui/*"
      ],
      "@infra/*": [
        "./infra/*"
      ],
      "@data/*": [
        "./data/*"
      ],
      "@domain/*": [
        "./domain/*"
      ],
      "@res/*": [
        "./res/*"
      ]
    },
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}